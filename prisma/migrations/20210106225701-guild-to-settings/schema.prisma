// This is your Prisma schema file,
datasource db {
	provider	= "postgresql"
	url = "***"
}

generator client {
	provider	= "prisma-client-js"
}

model Settings {
	id				String @id @default(uuid())
	guildId			String @unique
	prefix			String @default("!")
}

model Pet {
	id				String @id @default(uuid())
	userId			String
	guildId			String
	name			String
	kc				Int
}