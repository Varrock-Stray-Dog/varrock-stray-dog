// This is your Prisma schema file,
datasource db {
	provider	= "postgresql"
	url = "***"
}

generator client {
	provider	= "prisma-client-js"
}

model Guild {
	id				String @id @default(uuid())
	discord_id		String @unique
	prefix			String @default("!")
}

model Pet {
	id				String @id @default(uuid())
	user_id			String
	guild_id		String
	name			String
	kc				Int
}